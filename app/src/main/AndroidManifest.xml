        </activity>
        <activity android:name=".activity.MainActivity" >
            <intent-filter>
                <action android:name="com.example.chemss.autoecolee.activity.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegisterActivity" >
            <intent-filter>
                <action android:name="com.example.chemss.autoecolee.activity.REGISTERACTIVITY" />

