        </activity>
        <activity android:name=".activity.Accueil" >
            <intent-filter>
                <action android:name="com.example.chemss.autoecolee.ACCUEIL" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Inscription" >
            <intent-filter>
                <action android:name="com.example.chemss.autoecolee.INSCRIPTION" />

                <category android:name="android.intent.category.DEFAULT" />
